<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?scenebuilder-preview-i18n-resource SettingsView_en.properties?>

<AnchorPane id="AnchorPane" maxHeight="-1.0" maxWidth="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml">
  <children>
    <VBox id="VBox" alignment="TOP_CENTER" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <AnchorPane padding="$x1" prefHeight="45.0" prefWidth="-1.0" VBox.vgrow="NEVER">
          <children>
            <HBox id="HBox" alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <children>
                <Label id="titleLabel" alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" text="%label.tittle.text" HBox.hgrow="ALWAYS" />
              </children>
              <padding>
                <Insets fx:id="x1" />
              </padding>
            </HBox>
          </children>
        </AnchorPane>
        <AnchorPane id="AnchorPane" padding="$x1" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
          <children>
            <VBox id="VBox" alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <children>
                <Label alignment="CENTER" text="%label.name.text" VBox.vgrow="ALWAYS">
                  <labelFor>
                    <TextField fx:id="name" alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" promptText="" text="Server name" VBox.vgrow="ALWAYS" />
                  </labelFor>
                </Label>
                <fx:reference source="name" />
                <AnchorPane id="AnchorPane" padding="$x1" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
                  <children>
                    <VBox id="VBox" alignment="CENTER" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                      <children>
                        <Pane prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
                        <Separator prefWidth="-1.0" />
                        <Label text="%label.sharingMode" />
                        <RadioButton fx:id="fullAccess" alignment="CENTER" contentDisplay="LEFT" mnemonicParsing="false" prefHeight="-1.0" prefWidth="200.0" text="%checkbox.fullaccess.text" VBox.vgrow="ALWAYS">
                          <toggleGroup>
                            <ToggleGroup fx:id="group" />
                          </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="viewAccess" alignment="CENTER" mnemonicParsing="false" prefWidth="200.0" text="%checkbox.viewaccess.text" toggleGroup="$group" VBox.vgrow="ALWAYS" />
                        <Separator prefWidth="200.0" />
                        <Pane prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
                      </children>
                    </VBox>
                  </children>
                </AnchorPane>
              </children>
              <padding>
                <Insets left="30.0" right="30.0" />
              </padding>
            </VBox>
          </children>
        </AnchorPane>
        <AnchorPane id="AnchorPane" padding="$x1" prefHeight="-1.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
          <children>
            <VBox id="VBox" alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <children>
                <CheckBox id="autoconnect" fx:id="autoConnect" mnemonicParsing="false" prefWidth="-1.0" text="%checkbox.autoconnect.text" />
                <Pane prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
              </children>
            </VBox>
          </children>
        </AnchorPane>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@Javnce.css" />
  </stylesheets>
</AnchorPane>
